{
  "name": "multi-cascader-base-ele",
  "version": "1.0.16",
  "description": "基于element-ui实现的多选级联组件，完全继承element-ui级联组件功能",
  "main": "dist/index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:component": "cross-env MODE=production webpack --config ./build/webpack.config.prod.js --progress --colors",
    "build:example": "cross-env MODE=production webpack --config ./build/webpack.config.prod.example.js --progress --colors",
    "build": "rm -rf ./dist && npm run build:example && npm run build:component",
    "dev": "cross-env MODE=development webpack-dev-server --config ./build/webpack.config.dev.js --progress --colors"
  },
  "keywords": [
    "eleme",
    "element-ui",
    "多选级联",
    "multiCascader",
    "cascader",
    "vue",
    "components"
  ],
